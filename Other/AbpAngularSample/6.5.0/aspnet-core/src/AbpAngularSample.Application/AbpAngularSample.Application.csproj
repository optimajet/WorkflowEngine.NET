<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <VersionPrefix>1.0.0.0</VersionPrefix>
    <TargetFramework>net5.0</TargetFramework>
    <AssetTargetFallback>$(AssetTargetFallback);portable-net45+win8+wp8+wpa81;</AssetTargetFallback>
    <AssemblyName>AbpAngularSample.Application</AssemblyName>
    <PackageId>AbpAngularSample.Application</PackageId>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <RootNamespace>AbpAngularSample</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\AbpAngularSample.Core\AbpAngularSample.Core.csproj" />
  </ItemGroup>

  <!--WorkflowEngineSampleCode-->
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Common" Version="3.8.0" />
    <PackageReference Include="WorkflowEngine.NETCore-Core" Version="5.1.7" />
    <PackageReference Include="WorkflowEngine.NETCore-ProviderForMSSQL" Version="5.1.7" />
  </ItemGroup>
  
  <ItemGroup>
    <Folder Include="DataAccess" />
  </ItemGroup>
</Project>